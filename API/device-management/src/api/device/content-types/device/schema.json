{
  "kind": "collectionType",
  "collectionName": "devices",
  "info": {
    "singularName": "device",
    "pluralName": "devices",
    "displayName": "Device",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "text",
      "required": true,
      "unique": false
    },
    "code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "active",
        "inactive"
      ],
      "required": true
    },
    "address": {
      "type": "text",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "devices"
    }
  }
}
